<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>Sentinel</PackageId>
		<Version>1.0.0</Version>
		<Authors>Kornidesz Máté</Authors>
		<Company>Mapex</Company>
		<Description>A high performance logging libarry.</Description>

		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/Kornimate/sentinel</RepositoryUrl>
		<PackageTags>utility;logging</PackageTags>

		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
	</PropertyGroup>

</Project>
